  <!-- Variables -->
  <message key="variable.document.label">Documento</message>
  <message key="variable.document.description">Si esta tarea está relacionada con un documento, Usted puede especificarlo aquí.</message>
  <message key="variable.details.label">Descripción detallada</message>
  <message key="variable.details.description">Explique aquí here lo que debia ejecutarse.</message>
  <message key="variable.workerComment.label">Comentario del ejecutor</message>
  <message key="variable.workerComment.description">Algunos comentarios de la persona que ejecuta la tarea.</message>
  <message key="variable.feedbackComment.label">Retroalimentación</message>
  <message key="variable.needsConfirmation.label">Requiere confirmación</message>
  <message key="variable.needsConfirmation.description">Habilite este escaque si desea ser notificado (en su carpeta de entrada de tareas) cuando la tarea ha terminado.</message>
  <message key="variable.worker.label">Ejecutante de la tarea</message>
  <message key="variable.reviewSubmitComment.label">Comentario para la revisión</message>

