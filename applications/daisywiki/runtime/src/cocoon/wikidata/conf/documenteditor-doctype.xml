<!--
  This is a sample configuration.
  Configuration is on a per document type basis.
  Change the name of the file so it has the following format 'documenteditor-[documentTypeName]'
-->
<documenteditor-doctype>
  <!-- 
      Tab configuration.
      Change the sequence of tabs or leave out tab pages
  -->
  <tab-sequence>
    <tab>parts</tab>
    <tab>fields</tab>
    <tab>links</tab>
    <tab>misc</tab>
  </tab-sequence>
</documenteditor-doctype>
